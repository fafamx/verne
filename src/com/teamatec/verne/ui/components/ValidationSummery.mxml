<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<!--- Defines the appearance of the Form class's background. -->
	<s:Rect id="background" left="0" right="0" top="0" bottom="0">
		<s:fill>
			<!--- @private -->
			<s:BitmapFill id="bgFill" source="@Embed('resources/images/stripe-yellow.png')" fillMode="repeat" smooth="true" rotation="90"/>
		</s:fill>
	</s:Rect>
	<s:Rect left="0" right="0" height="1" bottom="0">
		<s:fill>
			<!--- @private -->
			<s:SolidColor color="#dddddd"/>
		</s:fill>
	</s:Rect>
	<s:Rect left="0" right="0" height="1" top="0">
		<s:fill>
			<!--- @private -->
			<s:SolidColor color="#dddddd"/>
		</s:fill>
	</s:Rect>
</s:Group>
